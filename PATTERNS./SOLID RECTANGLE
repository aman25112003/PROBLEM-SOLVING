#include <iostream>
using namespace std;
int main() {
  int rows,columns;
  cout<<"ENTER THE NUMBER OF ROWS"<<endl;
  cin>>rows;
  cout<<"ENTER THE NUMBER OF COLUMNS"<<endl;
  cin>>columns;
    for(int i=0;i<rows;i++)
      {
        for(int j=0;j<columns;j++)
          {
            cout<<"*";
          }
        cout<<endl;
      }
  return 0;
}
