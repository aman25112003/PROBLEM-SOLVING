#include <iostream>
using namespace std;
int main() 
{
  int count=3;
  //PRINTING UPPER PART
  for(int i=0;i<4;i++)
    {
      for(int j=0;j<i+1;j++)
        {
          cout<<count;
        }
      count++;
      cout<<endl;
    }
  int count2=5;
  //PRINTING LOWER PART

  for(int i=0;i<3;i++)
    {
      for(int j=3;j>=i+1;j--)
        {
           cout<<count2;     
        }
      count2--;
      cout<<endl;
    }
  return 0;
}
