#include <iostream>
using namespace std;
int main() 
{

  int num;
  cout<<"ENTER THE NUMBER"<<endl;
  cin>>num;

  for(int i=0;i<num;i++)
    {
      for(int j=i+1;j<=num;j++)
        {
          if(j==i+1 || j==num || i==0)
          {
            cout<<j;
          }
          else
          {
            cout<<" ";
          }
        }
      cout<<endl;
    }
  return 0;
}
